{
  "xsappname": "Sathwik",
  "tenant-mode": "dedicated",
  "welcomeFile": "Sathwik/webapp/",
  "authenticationMethod": "route",
  "sessionTimeout": 100,
  "pluginMetadataEndpoint": "/metadata",
  "routes": [
    {
      "source": "^/Sathwik/webapp/(.*)",
      "target": "$1",
      "localDir": "Sathwikwebapp/",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    }
  ]
}
